; Stand
[Statedef 0]
type = S
physics = S
ctrl = cond(roundstate < 2,0,1)
sprpriority = 0

[State 0, DestroySelf]
type = DestroySelf
trigger1 = ishelper

[State 0, 1]
type = ChangeAnim
trigger1 = (Anim != 0 && Anim != 5 && Anim != 2000 && Anim != 2005 && Anim != 11000 && Anim != 11005 && Anim != 13000 && Anim != 13005 && Anim != 15000 && Anim != 15005 && Anim != 17000 && Anim != 17005 && Anim != 19000 && Anim != 19005)
trigger2 = (Anim = 5 || anim = 2005 || anim = 11005 || anim = 13005|| anim = 15005 || anim = 17005 || anim = 19005) && AnimTime = 0
value = 0+var(27)

[State 0, PosSet]
type = PosSet
trigger1 = 1
y = 0
;ignorehitpause = 
;persistent = 
[State -3, Turn]
type = ChangeAnim
trigger1 = var(27) != 0
trigger1 = anim = 5
value = 2005



[State -1, B]
